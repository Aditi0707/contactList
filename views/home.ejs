<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= title%>
    </title>
    <!-- <link rel="stylesheet" href="/css/home.css"> -->
    <link rel="stylesheet" href="/css/home.css">
</head>
<body>
    <h1 id="heading">My Contacts List</h1>
<div id="contact">
    <ul>


        <% for(let i of contact_list){%>
            <li id="list">
                <p><%=i.Name%></p>
                <p><%=i.phone%></p>
               <div class="delete"><a href="/delete-contact/<%= i.phone %>">delete</a></div>

            </li>
          
            
            <%}%>


    </ul>

</div>

    <form action="/create-contact" method="post">

        <input type="text" name="Name" placeholder="Enter name " required>
        <input type="number" name="phone" placeholder="Enter phone no." required>
        <button type="submit">Submit</button>

    </form>

  
<script src="/js/home.js"></script>
</body>
</html>